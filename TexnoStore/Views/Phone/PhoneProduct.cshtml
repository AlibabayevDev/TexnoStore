											function AddToCardQuick(elem) {
												var id = $("#stdProductId").val();
												var count = $("#stdCount").val();
												var obj = {
													ProductId: id,
													Count:count
												}
												$.ajax({
													url: "/Home/AddToCard",
													method: "POST",
													data: obj,
													success: function (data) {
														if(data)
														{
															$('#createModal').html(data);
															$("#createModal").modal("show");
														}
														else{
															window.location.href= "/Account/Index";														
														}
													},
